    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug CronCalcTest",
            "type": "cppdbg",
            "request": "launch",
            "windows": {
                "MIMode": "gdb",
                "miDebuggerPath": "c:\\msys64\\mingw64\\bin\\gdb.exe",
                "program": "${workspaceFolder}\\.cmake\\cron_calc_test.exe",
                "cwd": "${workspaceFolder}\\.cmake",
                "environment": [
                    {
                        "name" : "PATH",
                        "value" : "c:\\msys64\\mingw64\\bin"
                    }
                ],
            },
            "args": [],
            "stopAtEntry": false,
            "externalConsole": false,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
        }
    ]
}
